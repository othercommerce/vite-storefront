    "keywords": ["othercommerce", "vite", "vue"],
    "version": "0.1.2",
    "license": "MIT",
    "homepage": "https://github.com/othercommerce/vite-storefront",
    "repository": {
        "type": "git",
        "url": "https://github.com/othercommerce/vite-storefront.git"
    },
    "bugs": {
        "url": "https://github.com/othercommerce/vite-storefront/issues",
        "email": "rafal.krawiec@promoznawcy.pl"
    },
    "author": {
        "name": "Rafa≈Ç Krawiec",
        "email": "rafal.krawiec@promoznawcy.pl"
    },
    "type": "module",
    "module": "dist/vite-storefront.js",
    "main": "dist/vite-storefront.cjs",
    "types": "dist/vite-storefront.d.ts",
    "files": ["dist"],
